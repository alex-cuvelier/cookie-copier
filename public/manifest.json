{
  "manifest_version": 3,
  "name": "Cookie Copier",
  "description": "This extension copies cookies from one domain to another.",
  "version": "1.0",
  "permissions": ["cookies", "tabs"],
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}