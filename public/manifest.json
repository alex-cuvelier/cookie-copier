{
    "manifest_version": 2,
    "name": "Cookie Copier",
    "description": "This extension copies cookies from one domain to another.",
    "version": "1.0",
    "permissions": ["cookies", "<all_urls>", "tabs"],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }